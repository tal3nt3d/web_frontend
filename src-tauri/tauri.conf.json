{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "web_frontend",
  "version": "0.1.0",
  "identifier": "com.tal3nt3d.web-frontend",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "https://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "plugins": {
    "http": {
        "enabled": true
      }
  },
  "app": {
    "withGlobalTauri": false
    ,
    "windows": [
      {
        "title": "web_frontend",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": "default-src 'self' http://172.27.61.159:8080; connect-src 'self' http://172.27.61.159:8080;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
